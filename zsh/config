autoload -U colors
colors

fpath=(~/.zsh/functions $fpath)
autoload -U ~/.zsh/functions/*(:t)

autoload -U url-quote-magic
zle -N self-insert url-quote-magic

REPORTTIME=10

setopt AUTO_CD
setopt CORRECT

setopt HIST_VERIFY
setopt INC_APPEND_HISTORY
setopt HIST_IGNORE_ALL_DUPS
setopt HIST_REDUCE_BLANKS

bindkey -e
